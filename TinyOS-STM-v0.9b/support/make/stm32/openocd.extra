PROGRAM = stm32-p103-ocd-program.py

PROGRAMMER ?= $(TOSROOT)/tools/platforms/stm32-p103/openocd/stm32-p103-ocd-program.py

program: FORCE
	@echo "    installing $(PLATFORM) binary using $(PROGRAM)"
	$(PROGRAMMER) $(INSTALL_BIN)

program_input: bin
	@:

